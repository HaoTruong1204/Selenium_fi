# Cấu hình ứng dụng
APP_NAME=Selenium Automation Hub
APP_VERSION=1.0.0
ORGANIZATION_NAME=GDU

# Cấu hình môi trường
ENABLE_HEADLESS=False
ENABLE_STEALTH_MODE=True
ENABLE_PROXY_ROTATION=False

# Cấu hình CAPTCHA
CAPTCHA_SERVICE=manual
CAPTCHA_API_KEY=

# Thông tin đăng nhập Facebook (thay thế bằng thông tin thật khi cần)
FACEBOOK_EMAIL=your_email@example.com
FACEBOOK_PASSWORD=your_password

# Cấu hình chung
DEFAULT_RETRY=3
DEFAULT_TIMEOUT=30

# Cấu hình Brave Browser
BRAVE_PATH=C:\Program Files\BraveSoftware\Brave-Browser\Application\brave.exe
BRAVE_PROFILE_PATH=C:\Users\admin\AppData\Local\BraveSoftware\Brave-Browser\User Data\Default 